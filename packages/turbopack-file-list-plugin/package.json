{
  "name": "turbopack-file-list-plugin",
  "version": "1.0.0",
  "description": "A plugin for Next.js with Turbopack to generate a list of all files used in the build",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./client": "./dist/client.js",
    "./dist/runtime": "./dist/runtime.js",
    "./dist/file-tracker-loader": "./dist/file-tracker-loader.js",
    "./dist/text-extraction-loader": "./dist/text-extraction-loader.js"
  },
  "bin": {
    "turbopack-file-list-plugin": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "nextjs",
    "turbopack",
    "plugin",
    "build"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/generator": "^7.28.5",
    "@babel/parser": "^7.28.5",
    "@babel/traverse": "^7.28.5",
    "@babel/types": "^7.28.5",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__traverse": "^7.28.0",
    "@types/node": "^22.0.0",
    "typescript": "^5.6.0"
  },
  "peerDependencies": {
    "next": ">=14.0.0",
    "react": "^18.0.0"
  }
}
